<%= provide(:title, "Current Job") %>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <% if @job.nil? %>
    <div class="panel panel-default">
      <div class="panel-heading">Info</div>
      <div class="panel-body"><p>You have no in-complete job.</p></div>
    </div>
    <% else %>
      <%= render 'current_job_info' %>
    <% end %>
  </div>
</div>
